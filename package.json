{
  "name": "hoffmation-base",
  "description": "Base Libraries and functions for own Hoffmation projects",
  "version": "2.6.2",
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theimo1221/Hoffmation-Base.git"
  },
  "author": "Thiemo Hoffmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theimo1221/Hoffmation-Base/issues"
  },
  "homepage": "https://github.com/theimo1221/Hoffmation-Base#readme",
  "scripts": {
    "jest": "jest",
    "jest-coverage": "jest --coverage",
    "lint-check-all": "eslint ./**/{*.js,*.ts,*.test.ts} --no-error-on-unmatched-pattern",
    "lint-fix-all": "eslint ./**/{*.ts,*.test.ts} --fix --no-error-on-unmatched-pattern",
    "build": "tsc",
    "prepare": "npm run build",
    "full-test": "npm run lint-check-all && npm run jest-coverage",
    "test": "npm run lint-check-all && npm run jest",
    "release": "release-script",
    "check-package-upgrades": "npx npm-check-updates -i",
    "pre-release-check": "npm run build && npm run full-test",
    "release-patch-pre": "npm run pre-release-check && npm run release prerelease --",
    "release-patch": "npm run pre-release-check && npm run release patch --",
    "release-minor": "npm run pre-release-check && npm run release minor --",
    "release-major": "npm run pre-release-check && npm run release major --",
    "manual-test-iobroker-connection": "tsc && ts-node ./manualTests/io-connection-test.ts",
    "manual-test-samsungTV": "tsc && ts-node ./manualTests/samsung-tv-test.ts"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@svrooij/sonos": "^2.5.0",
    "@types/debug": "^4.1.7",
    "@types/iobroker": "^4.0.5",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.15.0",
    "@types/node-telegram-bot-api": "^0.61.3",
    "@types/pg": "^8.6.6",
    "@types/socket.io-client": "^1.4.36",
    "@types/xml2js": "^0.4.11",
    "aws-sdk": "2.1338.0",
    "bson": "^5.0.1",
    "daikin-controller": "^2.1.0",
    "get-mp3-duration": "^1.0.0",
    "https": "^1.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "3.3.0",
    "node-ical": "^0.16.0",
    "node-merlin-wrt-api": "^0.0.2-alpha.5",
    "node-telegram-bot-api": "^0.61.0",
    "pg": "^8.10.0",
    "rss-parser": "^3.12.0",
    "samsung-tv-control": "^1.13.0",
    "socket.io-client": "2.5.x",
    "suncalc": "1.9.0",
    "sunrise-sunset-js": "^2.2.1",
    "tibber-api": "^5.1.8"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.5.9",
    "@types/node-ssdp": "^4.0.1",
    "@types/suncalc": "^1.9.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "coveralls": "^3.1.1",
    "eslint": "8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
